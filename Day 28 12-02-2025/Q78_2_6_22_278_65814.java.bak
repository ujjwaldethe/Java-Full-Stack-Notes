import java.util.Scanner;
class Sample2 
{
	public static void main(String[] args) 
	{
		int two = 2;
		int pow = 1 ;
		long prev = 2;
		System.out.print("\nEnter Range : ");
		int n = new Scanner(System.in).nextInt();
		int powRes = 2;
		
		for(int i=0; i<n;i++)
		{
			for(int j = 1 ; j<=pow;j++)
			{
				powRes*=two;
			}	
			System.out.print(prev+" ");
			prev=powRes+prev;
			pow*=2;
		}
	}
}
