class LongNarrowing 
{
	public static void main(String[] args) 
	{
		long a = 2343434.0f;
		long c = (long) a;
		System.out.println(c);//2343434
		int d = (int) a;
		System.out.println(d);//2343434.0
		short e = (short)a;
		System.out.println(e);//-15862
		char f = (char)a;
		System.out.println(f);//?
		byte g = (byte)a;
		System.out.println(g);//10

	}
}
