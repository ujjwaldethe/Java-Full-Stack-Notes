import java.util.Scanner;
class Q72PrimePlusAndMinusPrint
{
	public static void main(String[] args) 
	{
		boolean flag = false;
		Scanner sc = new Scanner(System.in);
		System.out.println("\nEnter Number : ");
		int num = sc.nextInt();
		int temp = num;
		int sub = num;
		int add1,add2;
		num--;
		temp++;
		for(int i = 2 ; i<num ; i++)
		{
			flag = false;
			if(num%i==0)
				flag=true;
			if(flag)
			{
				i = 2;
				num--;
			}
		}
		for(int i = 2 ; i<temp ; i++)
		{
			flag = false;
			if(temp%i==0)
				flag=true;	
			if(flag)
			{
				i = 2;
				temp++;
			}
		}
		add1 = sub-num; // subtract 
		add2 = temp-sub; //subtract
		if(add1<add2)
			System.out.println("Prime Number : "+num);
		else if(add2<add1)
			System.out.println("Prime Number : "+temp);
		else if(add1==1)
			System.out.println("Prime Number :"+temp);
		else if(add1==add1)
			System.out.println("Prime Number : "+num+" "+temp);
		if(num==1)
			System.out.println("Previous Prime Number : "+0+" Next Prime Number : "+temp);
		else
			System.out.println("Previous Prime Number : "+num+" Next Prime Number : "+temp);
	}
}
